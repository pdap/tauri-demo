{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "path:default",
    "event:default",
    "window:default",
    "app:default",
    "image:default",
    "resources:default",
    "menu:default",
    "tray:default",
    "shell:allow-open",
    "fs:read-all",
    "fs:write-all",
    "fs:allow-home-read-recursive",
    "fs:allow-home-write-recursive",
    "fs:read-dirs",
    "dialog:allow-open",
    "dialog:allow-save"
    
  ]
}